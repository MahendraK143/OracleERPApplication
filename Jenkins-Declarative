pipeline {
   agent any
     
   environment {
      PATH = "%M2_HOME%\bin"
   }
   
   stages {
      stage('Hello') {
         steps {
            echo 'Hello World'
         }
      }
      stage('Git Checkout') {
         steps {
            git credentialsId: 'GITCreds', url: 'https://github.com/MahendraK143/OracleERPApplication'
         }
      }
      stage('Maven Build') {
         steps {
            echo "cd OracleERPApplication"
            echo "mvn clean install"
         }
      }
     
   }
}
