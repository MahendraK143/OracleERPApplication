pipeline {
   agent any
   environment {
		PATH = "/opt/maven3/bin:$PATH"
	}

   stages {
      stage('Hello') {
         steps {
            echo 'Hello World'
         }
      }
      stage('Git Checkout') {
         steps {
            git credentialsId: 'GITCreds', url: 'https://github.com/MahendraK143/spring-boot-eamples'
         }
      }
      stage('Maven Build'){
			steps{
					
				sh "mvn clean package"
			}
		}
   }
}
